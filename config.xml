<?xml version='1.0' encoding='utf-8'?>
<widget id="io.hrxclub.hrx" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>hrx</name>
    <description>
        Hack Reactor own alumni network
    </description>
    <author email="julesmoretti@me.com" href="http://jules.digital">
        Hack Reactor / Jules Moretti
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <feature name="Geolocation">
      <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="InAppBrowser">
      <param name="android-package" value="org.apache.cordova.InAppBrowser"/>
    </feature>
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="de.appplant.cordova.plugin.background-mode" />
</widget>
